{
  "name": "node-module-seed",
  "description": "A repo to kickstart your next typescript node module",
  "main": "compiled/src/index.js",
  "types": "compiled/src/index.d.ts",
  "version": "0.0.2",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "2.8.8",
    "grunt": "1.0.3",
    "grunt-cli": "1.2.0",
    "grunt-contrib-watch": "1.1.0",
    "grunt-exec": "3.0.0",
    "istanbul-reporter-shield-badge": "1.1.3",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "jasmine-terminal-reporter": "1.0.3",
    "nyc": "12.0.2",
    "ts-lint": "4.5.1",
    "ts-loader": "5.3.0",
    "typescript": "3.1.6",
    "webpack": "4.24.0",
    "webpack-cli": "3.1.2",
    "yargs": "4.8.0"
  },
  "scripts": {
    "init": "bash ./scripts/init.sh",
    "dev": "grunt dev",
    "test": "grunt coverage",
    "build": "bash ./scripts/build.sh",
    "coverage": "grunt coverage"
  },
  "nyc": {
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
      "compiled/src/**/*.js"
    ],
    "reporter": [
      "json",
      "text-summary"
    ]
  }
